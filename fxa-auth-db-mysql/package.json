{
  "name": "fxa-auth-db-mysql",
  "version": "0.0.0",
  "description": "MySQL backend for the fxa-auth-db-server",
  "main": "bin/server.js",
  "repository": "mozilla/fxa-auth-db-mysql",
  "bin": {
    "fxa-auth-db-mysql": "db_patcher.js"
  },
  "scripts": {
    "test": "grunt && ./scripts/tap-coverage.js test/backend test/local",
