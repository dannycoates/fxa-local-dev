  },
  "dependencies": {
    "bluebird": "2.1.3",
    "bunyan": "0.23.1",
    "convict": "0.4.2",
    "fxa-auth-db-server": "git://github.com/chilts/fxa-auth-db-server.git#reverse-with-tests",
    "mysql": "2.3.2",
    "request": "2.36.0"
  },
  "devDependencies": {
    "ass": "git://github.com/jrgm/ass.git#5be99ee",
    "grunt": "0.4.5",
    "grunt-contrib-jshint": "0.10.0",
    "grunt-copyright": "0.1.0",
    "grunt-nsp-shrinkwrap": "0.0.3",
